<div class="btn-group" role="group">
    <a class="btn btn-default" href="{{ "{{ path('" ~ route_name_prefix ~ "') }}" }}">
        Back to the list
    </a>
{% if ('edit' in actions) and (not hide_edit) %}
        <a class="btn btn-default" href="{{ "{{ path('" ~ route_name_prefix ~ "_edit', { 'id': entity."~ identifier ~" }) }}" }}">
            Edit
        </a>
{% endif %}
{% if ('delete' in actions) and (not hide_delete) %}
    {{ '{{ form(delete_form) }}' }}
{% endif %}
</div>
